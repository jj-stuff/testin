#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules/@nuxt/devtools/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules/@nuxt/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules/@nuxt/devtools/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules/@nuxt/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/@nuxt+devtools@1.7.0_rollup@4.34.8_vite@6.2.0_@types+node@22.13.5_jiti@2.4.2_terser@5.3_41eadd037fc329f7a73b61dd47956f36/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
