#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules/nuxt/bin/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules/nuxt/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules/nuxt/bin/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules/nuxt/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/nuxt@3.15.4_@parcel+watcher@2.5.1_@types+node@22.13.5_db0@0.2.4_ioredis@5.5.0_magicast@_76c014d09dd81d561b968bfa7481f8db/node_modules:/Users/heathcliff/Downloads/devsite/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nuxt.mjs" "$@"
else
  exec node  "$basedir/../../bin/nuxt.mjs" "$@"
fi
